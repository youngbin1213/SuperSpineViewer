<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
            xmlns="http://javafx.com/javafx"
            fx:controller="com.SoarCraft.SuperSpineViewer.Controllers.MainController"
            fx:id="rootPane"
            prefHeight="720.0" prefWidth="1280.0"
            stylesheets="@css/main.css" styleClass="rootPane">

    <VBox layoutX="227.0" layoutY="51.0"
          prefWidth="302.0" styleClass="sidebar"
          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">

        <padding>
            <Insets bottom="10.0" left="14.0" top="14.0"/>
        </padding>

        <Label graphicTextGap="14.0" styleClass="header" text="SuperSpineViewer"/>

        <HBox fx:id="navBar" styleClass="navbar"/>

        <StackPane fx:id="contentPane" prefWidth="302.0" VBox.vgrow="ALWAYS"/>
    </VBox>

    <StackPane fx:id="spineViewer" layoutX="252.0" layoutY="52.0" prefHeight="150.0" prefWidth="200.0"
               AnchorPane.topAnchor="51.0" AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor="252.0" AnchorPane.rightAnchor="10.0"/>
</AnchorPane>
